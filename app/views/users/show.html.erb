<% provide(:title, @user.username) %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
      	<% if !@user.image? %>
			<%= image_tag("default.png") %>
      	<% else %>
			<%= image_tag(@user.image_url) if @user.image? %>
      	<% end %>
        <%= @user.username %>
      </h1>
    </section>
  </aside>
</div>