<%= will_paginate @users %>
<ul id="index-user-article" class="users">
	<% @users.each do |user| %>
		<li>
			<%= image_tag(user.image_url) %>
			<%= link_to user.username, user %>
			<% if user.admin? %>
				| Admin
			<% end %>
			<% if current_user.admin? && !current_user?(user) %>
			  | <%= link_to user, class: "btn btn-danger", method: :delete,
			                                  data: { confirm: "You sure?" } do%>
			        delete
			     <% end %>
			<% end %>
		</li>
		<% end %>
</ul>
<%= will_paginate @users %>